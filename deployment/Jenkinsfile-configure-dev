node {
	properties([
		[$class: 'BuildDiscarderProperty', strategy: [$class: 'LogRotator', numToKeepStr: '10']]
	])

	stage('Clone') {
		dir('browse-app') {
			checkout scm
		}
	}

	stage('Configure Dev') {
        try {
			dir('browse-app') {
				 sh "kubectl -n spindev apply -f config/env/dev/config.yaml"
			}
        } catch (exc) {
            currentBuild.result = "FAILURE"
            throw exc
        } finally {

        }
    }
}
